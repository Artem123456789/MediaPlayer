<Application x:Class="MediaPlayer.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:MediaPlayer"
             StartupUri="views/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="resource_dictionaries/Buttons.xaml"/>
                <ResourceDictionary Source="resource_dictionaries/Slider.xaml"/>
                <ResourceDictionary Source="resource_dictionaries/VolumeSlider.xaml"/>
                <ResourceDictionary Source="resource_dictionaries/ToggleButton.xaml"/>
                <ResourceDictionary Source="resource_dictionaries/PlaylistExpander.xaml"/>
                <ResourceDictionary Source="resource_dictionaries/Window.xaml"/>
                <ResourceDictionary Source="resource_dictionaries/ListBoxItems.xaml"/>
                <ResourceDictionary Source="resource_dictionaries/TextBlocks.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
